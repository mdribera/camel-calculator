{
  "name": "camel-calculator",
  "version": "1.0.0",
  "description": "A Camel Up probability calculator designed to ruin fun.",
  "main": "src/server.js",
  "scripts": {
    "lint:es": "eslint src",
    "lint:pug": "pug-lint src",
    "lint": "npm-run-all lint:*",
    "start:dev": "nodemon --exec babel-node --watch src -e js,json,pug,html,css src/server.js",
    "start": "npm run start:dev",
    "test": "mocha test/**/*.spec.js --compilers js:babel-register"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "chalk": "^2.1.0",
    "dotenv": "^4.0.0",
    "ejs": "^2.5.7",
    "express": "4.14.0",
    "gulp-messenger": "0.25.3",
    "livereload": "^0.6.2",
    "lodash": "4.17.2",
    "mongoose": "^4.11.13",
    "pug": "^2.0.0-rc.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mdribera/camel-calculator"
  },
  "keywords": [
    "node",
    "heroku",
    "express",
    "camel up",
    "probability",
    "dice",
    "permutation",
    "algorithm"
  ],
  "engines": {
    "node": "7.5.0"
  },
  "babel": {
    "presets": [
      "env"
    ],
    "ignore": [
      "node_modules"
    ]
  },
  "author": "Mark Ribera <markdribera@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "6.18.2",
    "babel-eslint": "^8.0.0",
    "babel-preset-env": "^1.6.0",
    "eslint": "^4.7.1",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-plugin-import": "^2.7.0",
    "mocha": "3.1.2",
    "nodemon": "1.11.0",
    "npm-run-all": "^4.1.1",
    "pug-lint": "^2.5.0"
  },
  "bugs": {
    "url": "https://github.com/mdribera/camel-calculator/issues"
  },
  "homepage": "https://github.com/mdribera/camel-calculator#readme",
  "directories": {
    "test": "test"
  }
}
